type Timer {
    key: ID!

    dueDate(zoneId: String = "Z"): String
    repetitions: Int

    workflowInstance: WorkflowInstance
    elementInstance: ElementInstance

    workflow: Workflow

    state: TimerState!
    timestamp(zoneId: String = "Z"): String!
}

enum TimerState {
    CREATED
    TRIGGERED
    CANCELED
}


