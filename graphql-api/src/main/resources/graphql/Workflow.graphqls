type Workflow {
    key: ID!
    bpmnProcessId: String!
    version: Int!
    timestamp: Long!

    bpmnXML: String!

    workflowInstances: [WorkflowInstance!]

    timers: [Timer!]

    messageSubscriptions: [MessageSubscription!]
}

extend type Query {
    workflow(key: ID!): Workflow

    workflows(count: Int = 10, offset: Int = 0): [Workflow!]!
}
